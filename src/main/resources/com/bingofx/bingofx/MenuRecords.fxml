            <TableColumn fx:id="colnumpartida" prefWidth="100.0" text="PARTIDA" />
            <TableColumn fx:id="colnombrejugador" prefWidth="150.0" text="NOMBRE JUGADOR" />
            <TableColumn fx:id="coltiradaslinea" prefWidth="100.0" text="TIRADAS LINEA" />
            <TableColumn fx:id="coltiradasbingo" prefWidth="100.0" text="TIRADAS BINGO" />
         </columns>
      </TableView>
      <Button fx:id="btnFiltroLinea" layoutX="120.0" layoutY="325.0" mnemonicParsing="false" onAction="#FiltroLinea" text="Mejores jugadores (Linea)" />
      <Button fx:id="btnVerHistorial" layoutX="14.0" layoutY="325.0" mnemonicParsing="false" onAction="#FiltroHistorial" text="VER HISTORIAL" />
      <Button fx:id="btnFiltroBingo" layoutX="282.0" layoutY="325.0" mnemonicParsing="false" onAction="#FiltroBingo" text="Mejores jugadores (Bingo)" />
